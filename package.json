    },
    "license": "MIT",
    "engines": {
        "node": ">=22.0.0"
    },
    "private": true,
    "scripts": {
        "build": "rimraf ./dist && tsc -p . && tsc-alias",
        "lint": "eslint --cache --fix '**/*.ts'",
        "format": "prettier --config .prettierrc.json './src/**/*.{js,json,ts,md}' --write --log-level error",
        "test": "echo \"Error: no test specified\" && exit 1",
        "challenge": "ts-node ./src/index.ts"
    },
    "keywords": [
        "crossmint",
        "challenge"
    ],
    "dependencies": {
        "axios": "^1.8.4",
        "axios-retry": "^4.5.0",
        "dotenv": "^16.4.7",
        "zod": "^3.24.2"
